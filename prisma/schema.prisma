generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model strikes {
  id         BigInt    @id @default(autoincrement())
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  title      String?   @db.VarChar
  days       String[]
}

model projects {
  id          BigInt    @id @default(autoincrement())
  name        String
  description String
  created_at  DateTime? @default(now()) @db.Timestamptz(6)
}
